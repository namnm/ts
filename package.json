{
  "name": "ts",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "backend": "nodemon",
    "tsc": "tsc --noEmit && type-coverage",
    "gen": "graphql-codegen && prettier --write --loglevel=silent src/types.tsx",
    "pret": "eslint --fix --ignore-path=.gitignore --ext=.js,.tsx . && prettier --write --ignore-path=.gitignore --loglevel=silent \"**/*.{js,tsx,json,md}\""
  },
  "dependencies": {
    "colors": "1.4.0",
    "compression": "1.7.4",
    "cookie": "0.4.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-graphql": "0.9.0",
    "graphql": "14.6.0",
    "graphql-tools": "4.0.7",
    "reflect-metadata": "0.1.13",
    "sequelize": "5.21.5",
    "sequelize-typescript": "1.1.0",
    "stack-trace": "0.0.10"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-syntax-class-properties": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.9.0",
    "@babel/preset-react": "7.9.1",
    "@babel/preset-typescript": "7.9.0",
    "@babel/register": "7.9.0",
    "@graphql-codegen/add": "1.13.1",
    "@graphql-codegen/cli": "1.13.1",
    "@graphql-codegen/typescript": "1.13.1",
    "@types/bluebird": "3.5.30",
    "@types/compression": "1.7.0",
    "@types/cookie": "0.3.3",
    "@types/express": "4.17.3",
    "@types/node": "13.9.2",
    "@types/stack-trace": "0.0.29",
    "@types/validator": "12.0.1",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-simple-import-sort": "5.0.2",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "type-coverage": "2.4.2",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,tsx}": "eslint --fix",
    "*.{js,tsx,json,md}": "prettier --write"
  },
  "typeCoverage": {
    "atLeast": 100,
    "strict": true
  }
}
