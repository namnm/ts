{
  "name": "ts",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "backend": "nodemon",
    "tsc": "tsc --watch --noEmit --preserveWatchOutput false",
    "pret": "eslint --fix --ignore-path=.gitignore --ext=.js,.ts,.tsx . && prettier --loglevel=error --ignore-path=.gitignore --write \"**/*.{js,ts,tsx,json,md}\"",
    "drop": "node -r ./register ./sequelize/dropGuard && sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "postinstall": "node -r ./register ./sequelize/cliFixTs"
  },
  "dependencies": {
    "colors": "1.4.0",
    "compression": "1.7.4",
    "cookie": "0.4.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-graphql": "0.9.0",
    "graphql": "14.6.0",
    "graphql-tools": "4.0.7",
    "lodash": "4.17.15",
    "reflect-metadata": "0.1.13",
    "sequelize": "5.21.6",
    "sequelize-typescript": "1.1.0",
    "stack-trace": "0.0.10"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-syntax-class-properties": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@babel/register": "7.9.0",
    "@graphql-codegen/add": "1.13.2",
    "@graphql-codegen/cli": "1.13.2",
    "@graphql-codegen/typescript": "1.13.2",
    "@types/bluebird": "3.5.30",
    "@types/compression": "1.7.0",
    "@types/cookie": "0.3.3",
    "@types/express": "4.17.4",
    "@types/lodash": "4.14.149",
    "@types/node": "13.11.0",
    "@types/stack-trace": "0.0.29",
    "@types/validator": "12.0.1",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-simple-import-sort": "5.0.2",
    "husky": "4.2.3",
    "kill-port": "1.6.0",
    "lint-staged": "10.1.2",
    "mysql2": "2.1.0",
    "nodemon": "2.0.2",
    "prettier": "2.0.4",
    "sequelize-cli": "5.5.1",
    "type-coverage": "2.4.2",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "type-coverage && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix",
    "*.{js,ts,tsx,json,md}": "prettier --write"
  },
  "typeCoverage": {
    "atLeast": 100,
    "strict": true
  }
}
